{
    "version": 2,
    "builds": [
      {
        "src": "js/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "js/server.js" },
      { "src": "/css/(.*)", "dest": "/css/$1" },
      { "src": "/js/(.*)", "dest": "/js/$1" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
